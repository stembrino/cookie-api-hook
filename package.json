{
  "name": "cookie-api-hook",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Fabio Carvalho",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.{js,ts}' --max-warnings=0",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix --max-warnings=0",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",

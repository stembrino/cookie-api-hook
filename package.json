    "dev": "nodemon --exec ts-node src/server.ts"
  },
  "dependencies": {
    "@types/node": "^22.7.6",
    "fastify": "^5.0.0",
    "fs-extra": "^11.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  }
}
